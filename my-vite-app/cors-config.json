[
  {
    "origin": ["*"],
    "method": ["GET", "HEAD", "POST", "PUT", "DELETE", "OPTIONS"],
    "responseHeader": [
      "Content-Type", 
      "Content-Length", 
      "Cache-Control", 
      "Content-Language", 
      "Content-Encoding", 
      "Content-Range", 
      "Access-Control-Allow-Origin",
      "Access-Control-Allow-Headers",
      "Access-Control-Allow-Methods",
      "Authorization"
    ],
    "maxAgeSeconds": 3600
  }
]
